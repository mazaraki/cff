onsCamps[i][4]\n\n      latlngset = new google.maps.LatLng(lat, long);\n\n      var markerCamp = new google.maps.Marker({\n        position: latlngset,\n        map: map,\n        icon: pinImage4,\n        title: infos,\n        shadow: pinShadow4\n\n      });\n     \n      var content = \"<h4>Πληροφορίες: </h4>\" + infos  + \"<h4>Διεύθυνση: </h4> \" + add + \"<p><a href=\"+\">open here</a></p>\";   \n\n      var infowindow = new google.maps.InfoWindow()\n\n      google.maps.event.addListener(markerCamp, 'click',(function(markerCamp, content, infowindow){\n        return function(){\n          infowindow.setContent(content);\n          infowindow.open(map,markerCamp);\n        };\n      })(markerCamp,content,infowindow));\n   }//end of for()\n  } //end of setMarkers func\n\n  function setMarkersOfficial(map,locationsOfficial){\n    var markerOfficial, i\n\n    for (i = 0; i < locationsOfficial.length; i++) {  \n      var infos = locationsOfficial[i][0]\n      var telNo = locationsOfficial[i][2]\n      var onlineSite = locationsOfficial[i][3]\n      var lat = locationsOfficial[i][4]\n      var long = locationsOfficial[i][5]\n      var add =  locationsOfficial[i][1]\n\n      latlngset = new google.maps.LatLng(lat, long);\n\n      var markerOfficial = new google.maps.Marker({\n        position: latlngset,\n        map: map,\n        icon: pinImage3,\n        title: infos,\n        shadow: pinShadow3\n      });\n        \n      var content =  \"<h4>\" + infos + \"</h4>\"  + \"<h4>Τηλέφωνο επικοινωνίας: </h4>\" + telNo + \"<h4>Διεύθυνση: </h4> \" + add + \"<h4>Ιστότοπος: </h4>\" + onlineSite;   \n\n      var infowindow = new google.maps.InfoWindow()\n\n      google.maps.event.addListener(markerOfficial, 'click',(function(markerOfficial, content, infowindow){\n        return function(){\n          infowindow.setContent(content);\n          infowindow.open(map,markerOfficial);\n        };\n      })(markerOfficial,content,infowindow));\n    }//end of for()\n  } //end of set